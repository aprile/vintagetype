section.alum
  background-color $teal
  font-weight 800

  h1
    font-size 45px
    font-family $font-oswald
    text-transform uppercase
    letter-spacing 4px
    line-height 50px
    padding 15px 0px
  h2
    font-style italic
    font-size 30px
    padding-bottom 15px
  h3
    font-size 16px
    text-transform uppercase
    letter-spacing 1px
  h4
    padding-top 30px
    font-size 16px
    display inline-block
  h4:first-of-type
    padding-right 80px
    