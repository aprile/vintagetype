section.combinade
  background-color $blue
  color $paper
  
  h1
    font-family $font-herr
    font-size 120px
    font-weight normal
    border-bottom 6px solid $paper
    display inline-block
    line-height 80px
    padding-top 30px
    margin-bottom 20px
    
  h2
    font-family $font-raleway
    font-weight normal
    text-transform uppercase
    font-size 18px
    padding-bottom 10px
    letter-spacing 3px
    line-height 10px
    
  h3
    font-family $font-krona
    font-weight normal
    text-transform uppercase
    font-size 12px
    letter-spacing 2px
    line-height 30px
    
  h4
    font-family $font-krona
    font-size 9px 
    letter-spacing 1px
    font-weight normal
    letter-spacing 2px
    text-transform uppercase
    position absolute
    
  .tl
    -webkit-transform translate(0px) rotate(-25deg)
    -webkit-transform-origin 0
    transform translate(0px) rotate(-25deg)
    transform-origin 96px 110px
      
  .tr 
    -webkit-transform translate(0px) rotate(25deg)
    -webkit-transform-origin 0
    transform translate(0px) rotate(25deg)
    transform-origin 305px 530px
  
  .bl
    font-size 10px 
    letter-spacing 3px
    -webkit-transform translate(0px) rotate(25deg)
    -webkit-transform-origin 0
    transform translate(0px) rotate(25deg)
    transform-origin 100px -50px
     
  .br
    font-size 10px 
    letter-spacing 3px
    -webkit-transform translate(0px) rotate(-25deg)
    -webkit-transform-origin 0
    transform translate(0px) rotate(-25deg)
    transform-origin 290px -530px
      